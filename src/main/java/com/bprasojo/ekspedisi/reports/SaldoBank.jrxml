	<parameter name="p_tanggal" class="java.util.Date"/>
	<query language="SQL"><![CDATA[SELECT d.kode as kode_akun, d.nama as nama_akun, cb.bank_id, c.no_rekening, c.nama_bank, c.atas_nama, cb.saldo_akhir + COALESCE(SUM(v.kredit - v.debet), 0) AS saldo_akhir_per_bank
    FROM closing_bank cb
    LEFT JOIN v_mutasi_bank v ON v.bank_id = cb.bank_id AND v.tanggal BETWEEN cb.tanggal AND  $P{p_tanggal} 
		left join bank c on cb.bank_id = c.id
		left join perkiraan d on c.akun_id = d.id
    WHERE cb.tanggal <=  $P{p_tanggal} 
    GROUP BY d.kode , d.nama , cb.bank_id, c.no_rekening, c.nama_bank, c.atas_nama, cb.saldo_akhir
    order by d.kode,  c.no_rekening]]></query>
